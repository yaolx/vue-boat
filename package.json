    "@vue/compiler-sfc": "^3.0.4",
    "@vuedx/typecheck": "^0.3.1-insiders-1606311019.0",
    "@vuedx/typescript-plugin-vue": "^0.3.1-insiders-1606311019.0",
    "dotenv": "^10.0.0",
    "typescript": "^4.1.2",
    "vite": "^2.6.13",
    "vite-plugin-mock": "^2.9.6"
